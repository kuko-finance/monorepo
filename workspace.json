{
  "version": 2,
  "projects": {
    "kuko-app": {
      "root": "apps/kuko-app",
      "sourceRoot": "apps/kuko-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/kuko-app"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kuko-app/src/environments/environment.ts",
                  "with": "apps/kuko-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/kuko-app"
          },
          "configurations": {
            "production": {
              "buildTarget": "kuko-app:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
              "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
              "cwd": "apps/kuko-app"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/kuko-app"
          }
        }
      }
    },
    "kukore-react": {
      "root": "libs/kukore-react",
      "sourceRoot": "libs/kukore-react/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/kukore-react/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "kuko-app"
}